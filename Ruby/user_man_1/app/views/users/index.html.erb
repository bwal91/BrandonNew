<title>All Users</title>
<h1>Listing Users</h1>

<table>
	<tr>
		<th>First Name</th>
		<th>Last Name</th>
		<th align="left">Email</th>
	</tr>
	<% @users.each do |user| %>
	<tr>
		<td> <%= user.first_name %> </td>
		<td> <%= user.last_name %> </td>
		<td> <%= user.email %> </td>
	</tr>
	<% end %>
</table>
<div>
	<button><a href="/users/new/">New User</a></button>
</div>